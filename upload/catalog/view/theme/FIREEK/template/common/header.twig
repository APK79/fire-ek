<header>
  <div class="container">
    <div class="row head-menu">
      <div id="open-mode" class="col-sm-2 hidden-xs hidden-sm hidden-md"><p> {{ text_open }} </p><span> {{ open }} </span></div>
      <div id="words" class="col-sm-4 col-md-5 col-lg-3 hidden-xs"><span> {{ owner }} </span></div>
      <div class="col-lg-2 col-md-3 col-sm-4 col-xs-6">
    <div id="logo">
          {% if logo %}
            {% if home == og_url %}
              <span><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></span>
            {% else %}
              <a href="{{ home }}"><span><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></span></a>
            {% endif %}
          {% else %}
            <h1><a href="{{ home }}">{{ name }}</a></h1>
          {% endif %}
    </div>
      </div>
      <div id="telephone" class="col-lg-2 col-md-2 hidden-sm col-xs-6">
        <div class="pull-right hidden-xs">
        <p><a href="{{ contact }}"><span class="ti-location-pin"></span></a></p>
        <span><a href="tel:{{ telephone }}" target="_blank">{{ telephone }}</a></span></div>
      </div>
          <div id="header-links-groupe" class="col-sm-4 col-md-3">
            {{ search }}
            <div class="col-sm-4 col-md-3 visible-xs"><div class="links-position"><button type="button" class="btn btn-navbar navbar-toggle js-toggle-mobMenu" data-toggle="collapse" data-target=".navbar-ex1-collapse"><i class="fa fa-bars"></i></button></div></div>
            <div class="col-sm-4 col-md-3"><div class="links-position"><span class="btn-open-search"><svg class="icon-search"><use xlink:href="#search-btn"></use></svg></span></div></div>
            <div class="col-sm-4 col-md-3"><div class="links-position">
              <div class="links-groupe__account-item hidden-xs"><a href="{{ account }}" title="{{ text_account }}"><svg class="icon-user"><use xlink:href="#user-btn"></use></svg></a></div>
              <div class="links-groupe__account-mobile-item js-toggle-mob-acc visible-xs"><span href="{{ account }}" title="{{ text_account }}"><svg class="icon-user"><use xlink:href="#user-btn"></use></svg></span></div>
                <div class="wishlist" hidden="true"><span>{{ text_wishlist }}</span></div>
                <div class="head_account__mobile">
                <div class="popup-header visible-xs">
                  <a href="{{ account }}" title="{{ text_account }}"><svg class="icon-user"><use xlink:href="#user-btn"></use></svg><span>{{ text_account }}</span></a>
                  <span class="btn popup-close js-close-mob-acc"><svg class="close-button"><use xlink:href="#close-button"></use></svg></span>
                </div>
                <div class="head_account js-tabs-box hidden">
                  <div class="head_account__display">
                  {% if logged %}
                  <!--  whishlist header -->
                  {{ wish }}
                  <!-- end wishlist header -->
                  <div class="well">
                    <li class="hidden-xs"><a href="{{ account }}">{{ text_account }}</a></li>
                    <li><a href="{{ order }}">{{ text_order }}</a></li>
                    <li><a href="{{ transaction }}">{{ text_transaction }}</a></li>
                    <li><a href="{{ download }}">{{ text_download }}</a></li>
                    <li><a href="{{ logout }}">{{ text_logout }}</a></li>
                  </div>
                  {% else %}
                   <div class="well">
                    <div class="head_account__tabs">
                      <span class="head_account__tabs-item active js-tab">{{ text_login  }}</span>
                      <span class ="head_account__tabs-item js-tab">{{ text_register }}</span>
                    </div>
                    <form action="{{ action }}" method="post" enctype="multipart/form-data">
                      <div class="head_account__tabs-content-item js-tab-content active">
                        <div class="form-group">
                          <span class="ti-user"></span>
                          <input type="text" name="email" value="{{ email }}" placeholder=" " class="input-email form-control" />
                          <label class="control-label" for="input-email">{{ entry_email }}</label>
                          <i class="input-animation"></i>
                        </div>
                        <div class="form-group">
                          <span class="ti-key"></span>
                          <input type="password" name="password" value="{{ password }}" placeholder=" " class="input-password form-control" />
                          <label class="control-label" for="input-password">{{ entry_password }}</label>
                          <i class="input-animation"></i>
                          <a href="{{ forgotten }}">{{ text_forgotten }}</a></div>
                        <input type="submit" value="{{ button_login }}" class="btn btn-primary" />
                        {% if redirect %}
                        <input type="hidden" name="redirect" value="{{ redirect }}" />
                        {% endif %}
                      </div>
                    </form>
                    <div class="head_account__tabs-content-item js-tab-content">
                      <p>{{ text_register_account }}</p>
                      <a href="{{ register }}" class="btn btn-primary">{{ text_register }}</a>
                    </div>
                  </div>
                 {% endif %}
                 </div>
                </div>
              </div>
          </div>
        </div>
          <div class="col-sm-4 col-md-3"><div class="links-position">{{ cart }}</div></div>
      </div>
    </div>
    <div class="header-line"></div>
  </div>
</header>
{{ menu }}
